---
import BaseLayout from "../layouts/BaseLayout.astro";
import VideosComponent from "../components/Videos.astro";
import { getAllVideos } from "../utils/data";
const videos = await getAllVideos();
const { origin } = new URL(Astro.url.pathname, Astro.site);
---
<BaseLayout title={"Maria Due - Music Videos"}>
  <VideosComponent {videos} {origin} />
</BaseLayout>
